        <% if(typeof featuredArticlesRight != 'undefined'){ %>
        <% featuredArticlesRight.forEach(article =>{ %>
            
        
        <div class="single-blog-post small-featured-post d-flex">
            <div class="post-thumb">
                <a href="#"><img src="/img/bg-img/<%= article.article_image %>" alt=""></a>
            </div>
            <div class="post-data">
                <a href="/categories/<%=article.category_id %>" class="post-catagory"><%= article.category_title %></a>
                <div class="post-meta">
                    <a href="#" class="post-title">
                        <h6><%= article.article_text.substring(0, 100) %></h6>
                    </a>
                    <p class="post-date"><span>7:00 AM</span> | <span>April 14</span></p>
                </div>
            </div>
        </div>

        <% }); %>
        <% } %>
    </div>